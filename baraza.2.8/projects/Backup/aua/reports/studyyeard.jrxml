	<queryString>
		<![CDATA[SELECT b.major_name, b.student_year, count(b.student_degree_id) as students
FROM
(SELECT major_name, student_degree_id,
(CASE WHEN get_cumm_credit(student_degree_id, max_session_id) < 52 THEN '1. First Year'
WHEN get_cumm_credit(student_degree_id, max_session_id) < 103 THEN '2. Second Year'
WHEN get_cumm_credit(student_degree_id, max_session_id) < 154 THEN '3. Third Year' ELSE '4. Forth Year' END) as student_year
FROM
(SELECT major_name, student_degree_id, max(session_id) as max_session_id
FROM vw_sstudent_majors
WHERE (substring(session_id from 1 for 9) = '$P!{filterid}')
GROUP BY major_name, student_degree_id) as a) as b
GROUP BY b.major_name, b.student_year
ORDER BY b.major_name, b.student_year;]]>
	</queryString>
