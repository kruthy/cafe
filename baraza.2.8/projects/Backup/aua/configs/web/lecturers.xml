			<TEXTFIELD w="75" title="Ending" format="date">session_end</TEXTFIELD>
			<CHECKBOX w="90" title="active" format="boolean" ischar="true">active</CHECKBOX>
			<GRID noorg="true" name="Grade Book" keyfield="scourse_id" orderby="course_id" linkfield="session_id" table="vw_scourses" group="school_id" filterfunction="getSchoolID">
				<TEXTFIELD w="40" title="Semester">session_id</TEXTFIELD>
				<TEXTFIELD w="40" title="Course ID">course_id</TEXTFIELD>
				<TEXTFIELD w="200" title="Course Title">course_title</TEXTFIELD>
				<TEXTFIELD w="50" title="Option">class_option</TEXTFIELD>
				<TEXTFIELD w="50" title="Credit Hours">credit_hours</TEXTFIELD>
				<TEXTFIELD w="150" title="Instructor">instructor_name</TEXTFIELD>
				<JASPER name="Class List" showpdf="true" linkfield="filterid" reportfile="qgradeviewc.jasper"></JASPER>
				<JASPER name="Grade Report" showpdf="true" linkfield="filterid" reportfile="qgradeviewd.jasper"></JASPER>
			</GRID>
			<JASPER name="Probation List" showpdf="true" linkfield="filterid" group="department_id" reportfile="probationsc.jasper"></JASPER>
		</GRID>
	</DESK>
	<DESK  noorg="true" w="700" name="Post Graduate List" key="110" h="500">
		<GRID  name="Post Graduate List" keyfield="student_degree_id" table="vw_student_majors" noorg="true" where="(degree_level_id = 'PGD') or (degree_level_id = 'MAS') or (degree_level_id = 'PhD') or (degree_level_id = 'PHD')">
			<TEXTFIELD w="40" title="Student ID">student_id</TEXTFIELD>
			<TEXTFIELD w="200" title="Student Name">student_name</TEXTFIELD>
			<TEXTFIELD w="40" title="Sex">sex</TEXTFIELD>
			<TEXTFIELD w="200" title="Programme">major_name</TEXTFIELD>
			<JASPER name="Unofficial Transcript" showpdf="false" linkfield="filterid" reportfile="studentquarterview.jasper"></JASPER>
			<JASPER name="Check List" showpdf="false" linkfield="filterid" reportfile="studentdegreeviewb.jasper"></JASPER>
		</GRID>
	</DESK>
	<DESK noorg="true" w="700" name="Major Approvals" key="111" h="500">
		<GRID noorg="true" name="Major Approvals" keyfield="sstudent_id" table="vw_sstudent_majors" where="(active = true) AND (finalised = true) AND (major_approval = false) AND ((degree_level_id = 'PGD') or (degree_level_id = 'MAS') or (degree_level_id = 'PhD') or (degree_level_id = 'PHD'))">
			<TEXTFIELD w="40" title="Student ID">student_id</TEXTFIELD>
			<TEXTFIELD w="100" title="Student Name">student_name</TEXTFIELD>
			<TEXTFIELD w="30" title="Sex">sex</TEXTFIELD>
			<TEXTFIELD w="75" title="Major">major_name</TEXTFIELD>
			<EDITFIELD w="100" title="Not Approved" editkey="sstudent_id" editvalue="sstudents">no_approval</EDITFIELD>
			<ACTIONS>
				<ACTION fnct="updatemajorapproval">Approve</ACTION>
			</ACTIONS>
			<JASPER name="Unofficial Transcript" showpdf="false" linkfield="filterid" reportfile="studentquarterview.jasper"></JASPER>
			<JASPER name="Check List" showpdf="false" linkfield="filterid" reportfile="studentdegreeviewb.jasper"></JASPER>
			<JASPER name="Selections" showpdf="false" linkfield="filterid" reportfile="qstudentviewca.jasper"></JASPER>
		</GRID>
	</DESK>
</APP>
